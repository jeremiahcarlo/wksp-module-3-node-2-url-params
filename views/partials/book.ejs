<div class="container-book">
  <div class="book">
    <img src="<%= book.imgUrl%>">
    <p><span class="bold">Title: </span><%= book.title %> </p>
    <p><span class="bold">Author:</span> <%= book.author %></p>
    <p><span class="bold">Category:</span> <%= book.type %> </p>
    <p><span class="bold">Description: </span><%= book.description %> </p>
  </div>
</div>